{
  "private": true,
  "name": "care-ops-frontend",
  "sideEffects": false,
  "version": "0.1.0",
  "description": "Frontend Assets for RoundingWell Care Team Operations",
  "repository": "git@github.com:RoundingWell/care-ops-frontend.git",
  "author": "RoundingWell",
  "license": "CC-BY-4.0",
  "homepage": "https://RoundingWell.com",
  "scripts": {
    "analyze": "cross-env NODE_ENV=production webpack --profile --json > webpack.stats.json && webpack-bundle-analyzer webpack.stats.json dist/",
    "build": "cross-env NODE_ENV=production webpack",
    "coverage": "npm start & cypress run --env COVERAGE=true --config video=false && npm run coverage:report",
    "coverage:report": "nyc report --reporter=html --reporter=text",
    "coverage:report:ci": "nyc report --reporter=lcovonly || true",
    "dev": "webpack-dev-server --open",
    "dev:coverage": "npm start & cypress open --env COVERAGE=open",
    "kill": "pkill -f ./node_modules/.bin/webpack-dev-server",
    "lint": "eslint --fix src/js/ && eslint --fix test/",
    "start": "cross-env NODE_ENV=test webpack-dev-server --port 8090 --inline false"
  },
  "browserslist": [
    ">1%",
    "Explorer >= 11",
    "not op_mini all"
  ],
  "fontawesome": {
    "fas": [
      "faCaretLeft",
      "faCaretRight",
      "faCheckCircle",
      "faChevronLeft",
      "faChevronRight",
      "faDotCircle",
      "faExclamationCircle",
      "faInfoCircle",
      "faSignOutAlt",
      "faTimes"
    ],
    "far": [
      "faAngleDown",
      "faCalendarAlt",
      "faCheckCircle",
      "faEllipsisH",
      "faFileAlt",
      "faInfoCircle",
      "faPlusCircle",
      "faStopwatch",
      "faTimes",
      "faTimesCircle",
      "faTrashAlt",
      "faUserCircle"
    ]
  },
  "nyc": {
    "temp-directory": "./coverage",
    "report-dir": "./coverage",
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/js/base/helpers.js",
      "src/js/base/hotkeys.js",
      "src/js/services/auth.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@cypress/webpack-preprocessor": "^4.1.0",
    "@roundingwellos/babel-plugin-handlebars-inline-precompile": "^3.0.1",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^5.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "cypress": "^3.4.1",
    "eslint": "^6.2.2",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-mocha": "^6.1.0",
    "faker": "^4.1.0",
    "handlebars-template-loader": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "istanbul-lib-coverage": "^2.0.5",
    "js-yaml": "^3.13.1",
    "lodash.merge": "^4.6.2",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "null-loader": "^3.0.0",
    "nyc": "^14.1.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.3.1",
    "stylelint": "^10.1.0",
    "stylelint-bare-webpack-plugin": "^1.1.2",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-order": "^3.0.1",
    "stylelint-scss": "^3.10.0",
    "webpack": "^4.39.3",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-inject-plugin": "^1.5.3",
    "yaml-loader": "^0.5.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.22",
    "@fortawesome/pro-light-svg-icons": "^5.10.2",
    "@fortawesome/pro-regular-svg-icons": "^5.10.2",
    "@fortawesome/pro-solid-svg-icons": "^5.10.2",
    "@roundingwellos/libphonenumber-js": "^0.4.14",
    "animejs": "^3.1.0",
    "auth0-js": "^9.11.3",
    "backbone": "^1.4.0",
    "backbone.eventrouter": "^1.0.0",
    "backbone.marionette": "^4.1.0",
    "backbone.radio": "^2.0.0",
    "backbone.store": "^1.0.0",
    "core-js": "^2.6.9",
    "handlebars": "^4.1.2",
    "handlebars-intl": "^1.1.2",
    "jquery": "^3.4.1",
    "marionette.toolkit": "^6.1.0",
    "moment": "^2.24.0",
    "store": "^2.0.12",
    "underscore": "^1.9.1",
    "uuid": "^3.3.3"
  }
}
